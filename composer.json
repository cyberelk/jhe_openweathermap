{
  "name": "cyberelk/jhe_openweathermap",
  "type": "typo3-cms-extension",
  "description": "Displays the local weather data from https://openweathermap.org/",
  "authors": [
    {
      "name": "Jari-Herman Ernst",
      "email": "webdev@jhernst.de",
      "role": "Developer",
      "homepage": "https://www.jhernst.de"
    }
  ],
  "version": "v12.0.2",
  "require": {
    "typo3/cms-core": "~12.4"
  },
  "autoload": {
    "psr-4": {
      "Cyberelk\\JheOpenweathermap\\": "Classes/"
    }
  },
  "replace": {
    "cyberelk/jhe_openweathermap": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "jhe_openweathermap"
    }
  }
}
